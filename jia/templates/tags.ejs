<html>
  <%- include('./includes/head', { siteTitle: themeConfig.siteName }) %>
    <body class="mdui-theme-primary-purple mdui-theme-accent-purple">
        <header class="header mdui-m-b-3">
          <%- include('./includes/header') %>
        </header>
        <div class="mdui-container">
                <div class="mdui-row">
                        <div class="mdui-col-md-10 mdui-col-offset-md-1 ">
 
                            <div class="mdui-row-md-3 mdui-row-xs-1">
<% site.tags.forEach(function(tag) { %>
                                    <div class="mdui-col mdui-p-a-1">
                                       <article class="archives-list mdui-shadow-15  mdui-hoverable mdui-color-white ">
                                           <a href="<%= tag.link %>">
                                           <div class="mdui-p-a-2">
                                           <div class="mdui-typo-headline mdui-text-truncate index-list-title mdui-m-b-3"><%= tag.name %></div>
                                           <div class="mdui-typo-title mdui-m-t-1 mdui-text-color-blue-grey-200">共有<%= tag.count %>篇文章</div>
                                           </div>
                                        </a>
                                       </article> 
                                    </div>
<% }); %>
                                  </div>
                         
                        </div>
                 </div>
          
        </div>
        <%- include('./includes/footer') %>
    </body>
</html>